[185_HARD] Department Top Three Salaries

with temp as (select d.name as Department, 
                    e.name as Employee,
                    e.salary as Salary,
                    dense_rank() over(partition by e.departmentId order by salary desc) as salary_rnk 
            from employee e 
                join department d on e.departmentID = d.id )

select Department,
        Employee,
        Salary 
from temp 
where salary_rnk <= 3

